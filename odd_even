Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		for(int i=1;i<=n;i++) {
			int N = sc.nextInt();
			int rem=0;
			int even=0;
			int odd=0;
			while(N!=0) {
				rem = N%10;
				if ( rem%2==0)
				even+=rem;
				else
					odd+=rem;
					
				N=N/10;
			}	
			
		
			
			if( even%4==0 || odd%3==0) {
				System.out.println("Yes");
			}
			else {
				System.out.println("No");
			}
		
		}	
